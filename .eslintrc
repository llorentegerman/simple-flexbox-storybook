{
    "parser": "babel-eslint",
        "plugins": [
            "react"
        ],
            "extends": [
                "eslint:recommended",
                "plugin:react/recommended"
            ],
                "parserOptions": {
        "ecmaVersion": 6,
            "sourceType": "module",
                "ecmaFeatures": {
            "jsx": true,
                "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "es6": true
    },
    "rules": {
        "no-console": 0,
            "indent": ["error", 4, { "SwitchCase": 1 }],
                "semi": "error"
    }
}
